{
  "name": "@blizzbolts/vite-plugin-docit",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "default": "./build/index.cjs"
    }
  },
  "scripts": {
    "dev": "npm run build -- --sourcemap --watch ./src --watch ../shared/build",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "CONSOLA_LEVEL=4 vitest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "splitting": false,
    "shims": true,
    "clean": true,
    "outDir": "build",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@blizzbolts/docit-shared": "workspace:^",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "vite": "^4.4.9",
    "@mdx-js/rollup": "^2.3.0"
  }
}
