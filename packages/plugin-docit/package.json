{
  "name": "@blizzbolts/vite-plugin-docit",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "default": "./build/index.cjs"
    }
  },
  "scripts": {
    "dev": "npm run build -- --sourcemap --watch ./src --watch ../shared/build",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "CONSOLA_LEVEL=4 vitest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "splitting": false,
    "shims": true,
    "clean": true,
    "outDir": "build",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7"
  }
}
