{
  "name": "@blizzbolts/docit-shared",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "require": "./dist/index_node.js",
      "import": "./dist/index_node.mjs"
    },
    "./client": {
      "require": "./dist/index_client.js",
      "import": "./dist/index_client.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "./dist/index_node.d.ts"
      ],
      "client": [
        "./dist/index_client.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "run-p \"build --watch\"",
    "build": "tsup --entry.index_node src/index.node.ts --entry.index_client src/index.client.ts --entry.index src/index.ts --dts --format cjs,esm --no-splitting",
    "prepublishOnly": "npm run build",
    "test": "CONSOLA_LEVEL=4 vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/find-package-json": "^1.2.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.5",
    "fs-extra": "^11.1.1",
    "pkg-up": "^4.0.0",
    "type-fest": "^4.3.1"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "find-package-json": "^1.2.0"
  }
}
